{
    "version": 2,
    "builds": [
        {
            "src": "api.py",
            "use": "@vercel/python",
            "config": {
                "wsgiApplication": "api.app", 
                "runtime": "python3.9" 
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)", 
            "dest": "api.py"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}