{
    "version": 2,
    "builds": [
        {
            "src": "wsgi.py",
            "use": "@vercel/python",
            "config": {
                "wsgiApplication": "wsgi.app", 
                "runtime": "python3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)", 
            "dest": "wsgi.py"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}